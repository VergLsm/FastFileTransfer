<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--item android:drawable="@drawable/btn_open_pressed" android:state_pressed="true" /-->
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff009e00" />
            <corners android:radius="25dp" />
        </shape>
    </item>
    <!--item android:drawable="@drawable/btn_open_norm" android:state_pressed="false" /-->
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#ff00b400" />
            <corners android:radius="25dp" />
        </shape>
    </item>
</selector> 